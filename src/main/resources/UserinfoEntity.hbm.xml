<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.company.controller.UserinfoEntity" table="userinfo" schema="qline">
        <id name="userId">
            <column name="userID" sql-type="int(11)"/>
        </id>
        <property name="userLogin">
            <column name="userLogin" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="password">
            <column name="password" sql-type="varchar(45)" length="45"/>
        </property>
        <property name="firstName">
            <column name="firstName" sql-type="varchar(40)" length="40"/>
        </property>
        <property name="lastName">
            <column name="lastName" sql-type="varchar(50)" length="50"/>
        </property>
        <property name="phoneNumber">
            <column name="phoneNumber" sql-type="varchar(12)" length="12"/>
        </property>
    </class>
</hibernate-mapping>